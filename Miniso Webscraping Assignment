   "source": [
    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "import pandas as pd\n",
    "\n",
    "url = \"https://www.minisoindia.com/store-locator/\"\n",
    "# Make a GET request to the URL to retrieve the HTML code\n",
    "response = requests.get(url)\n",
    "html_content = response.content\n",
    "# Use BeautifulSoup to parse the HTML code\n",
    "soup = BeautifulSoup(html_content, \"html.parser\")\n",
    "# Find the dropdown menu containing the states\n",
    "state_dropdown = soup.find(\"select\", {\"id\": \"storelocation\"})\n",
    "\n",
    "# Create an empty DataFrame to store the data\n",
    "df = pd.DataFrame(columns=['State', 'Store Name', 'Address', 'Timings', 'Store Coordinates'])\n",
    "\n",
    "for state_option in state_dropdown.find_all('option'):\n",
    "    # Get the name of the current state option\n",
    "    state_name = state_option.get_text().strip()\n",
    "    # Get the value of the current state option\n",
    "    state_value = state_option['value']\n",
    "    # Construct the URL for the current state\n",
    "    state_url = f\"https://www.minisoindia.com/store-locator/?store_id={state_value}\"\n",
    "    # Make a GET request to the state URL\n",
    "    state_response = requests.get(state_url, headers={'Referer': 'https://www.minisoindia.com/store-locator/'})\n",
    "    state_soup = BeautifulSoup(state_response.content, \"html.parser\")\n",
    "    # Find all the store locations in the state\n",
    "    locations = state_soup.find_all(\"div\", {\"class\": \"store_locator\"})\n",
    "    # Loop through each store location in the state\n",
    "    for location in locations:\n",
    "        # Get the name of the store\n",
    "        store_name = location.find(\"strong\").text\n",
    "        # Get the address of the store\n",
    "        address = location.find_all(\"p\")[0].text\n",
    "        # Get the timings of the store\n",
    "        timings = location.find_all(\"p\")[1].text\n",
    "        # Get the coordinates of the store\n",
    "        store_coordinates = location.find(\"script\").text.split(\"=\")[-1].strip()[1:-2]\n",
    "        # Add the store details to the dataframe\n",
    "        df = df.append({'State': state_name, 'Store Name': store_name, 'Address': address, 'Timings': timings, 'Store Coordinates': store_coordinates}, ignore_index=True)\n",
    "\n",
    "# Print the dataframe\n",
    "print(df)"
   ]
