    "from geopy.geocoders import ArcGIS\n",
    "\n",
    "# create a geolocator object\n",
    "geolocator = ArcGIS(user_agent=\"my-application\")\n",
    "\n",
    "# loop over each row in the dataframe\n",
    "for index, row in df.iterrows():\n",
    "    # get the store name and state value for the row\n",
    "    location = row['Store Name + State']\n",
    "    \n",
    "    # use the geolocator to get the latitude and longitude coordinates\n",
    "    result = geolocator.geocode(location)\n",
    "    if result:\n",
    "        latitude = result.latitude\n",
    "        longitude = result.longitude\n",
    "    else:\n",
    "        latitude = None\n",
    "        longitude = None\n",
    "        \n",
    "    # add the latitude and longitude values as new columns in the dataframe\n",
    "    df.at[index, 'latitude'] = latitude\n",
    "    df.at[index, 'longitude'] = longitude"
   ]
